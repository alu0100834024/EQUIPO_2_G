import matplotlib.pyplot as hola
hola.subplot(2,1,1)
x=[0.496558178297, 2.13100384448, 0.689662720778, 0.739652997531, 0.739085204376, 0.739085133215, 0.739085133215, 0.739085133215]
y=[7.04158813835, 1.2330160875, 2.089972175491, 0.0675861206807, 0.000768237751393,9.62821076424e-08, 1.50215851291e-15, 0.0]
hola.plot(x,y,"r*")
hola.title('Grafica de errores ampliada')
hola.xlabel('x')
hola.ylabel('error')
hola.ylim(-0.4,7.3)
hola.xlim(0.48,2.2)

hola.subplot(2,1,2)
x=[0.739652997531, 0.739085204376, 0.739085133215, 0.739085133215, 0.739085133215]
y=[0.0675861206807, 0.000768237751393,9.62821076424e-08, 1.50215851291e-15, 0.0]
hola.plot(x,y,"yp")
hola.title('Grafica de errores reducida')
hola.xlabel('x')
hola.ylabel('error')
hola.ylim(-0.1,0.1)
hola.xlim(0.7390,0.7397)
hola.savefig("Grafica_de_errores.eps",dpi=72)
hola.show(y)
